{
  "build": {
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    }
  },

  "routes": [
    {
      "src": "/",
      "has": [
        { "type": "host", "value": "nextzerobar\\.org" },
        { "type": "host", "value": "www\\.nextzerobar\\.org" }
      ],
      "dest": "/app/platform",
      "status": 308
    },
    {
      "src": "/",
      "has": [
        { "type": "host", "value": "nextgroup\\.vercel\\.app" }
      ],
      "dest": "/app/int",
      "status": 308
    }
  ]
}
